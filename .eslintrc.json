{
    "extends":[
        "eslint:recommended",
        "plugin: import/errors",
        "plugin: react/recommend",
        "plugin: jsx-a11y/recommend",
        "plugin: react-hooks/recommend",
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0,
        "react/react-in-jsx-scope":0
    },
    "plugins": ["react","import","jsx-11y","react-hooks"],
    "parserOptions":{
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "setting": {
        "react": {
            "version":"detect"
        }
    }
}